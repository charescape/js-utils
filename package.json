{
  "name": "@charescape/js-utils",
  "amdName": "JsUtils",
  "version": "1.0.1",
  "description": "Lightweight javascript utilities for node.js and web",
  "keywords": [
    "lightweight",
    "utilities"
  ],
  "source": "src/index.js",
  "main": "dist/js-utils.js",
  "exports": "./dist/js-utils.modern.js",
  "module": "dist/js-utils.module.js",
  "umd:main": "dist/js-utils.umd.js",
  "files": [
    "dist"
  ],
  "author": "Charescape <charescape@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/charescape/js-utils"
  },
  "license": "MIT",
  "scripts": {
    "cypress:open": "cypress open",
    "build": "microbundle",
    "dev": "microbundle watch",
    "test": "jest",
    "redo-install": "rm -rf node_modules/ && rm -f package-lock.json && npm install --registry=https://registry.npmjs.org -dd",
    "do-npm-login": "npm login --registry=https://registry.npmjs.org -dd",
    "do-npm-publish": "npm publish . --access public --registry=https://registry.npmjs.org -dd"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "cypress": "^7.1.0",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0"
  }
